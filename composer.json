{
    "name": "vagner-giraldino/z-api-laravel-sdk",
    "description": "SDK Profissional para integração Z-API no Laravel com foco em remarketing e botões.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Vagner Giraldino",
            "email": "vgiraldino@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "illuminate/support": "^9.0|^10.0|^11.0",
        "guzzlehttp/guzzle": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "SuaEmpresa\\ZApi\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "SuaEmpresa\\ZApi\\ZApiServiceProvider"
            ],
            "aliases": {
                "ZApi": "SuaEmpresa\\ZApi\\Facades\\ZApi"
            }
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require-dev": {
        "pestphp/pest": "^4.3",
        "orchestra/testbench": "^7.0|^8.0|^9.0"
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
